{
  "name": "react-boilerplate",
  "version": "4.0.0",
  "description": "A minimal boilerplate with hot module replacement",
  "main": "./src/index.js",
  "scripts": {
    "start": "webpack-dev-server --config=webpack.dev.js",
    "build": "yarn run clean-dist && webpack -p --config=webpack.prod.js",
    "clean-dist": "rm -f -r -d dist",
    "start-prod": "yarn run build && node express.js"
  },
  "author": "bernardo de stefano",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git@github.com:bernardodestefano/react-boilerplate.git"
  },
  "keywords": [
    "frontend",
    "webpack",
    "boilerplate",
    "react",
    "babel",
    "sass",
    "hmr"
  ],
  "devDependencies": {
    "@babel/core": "^7.3.4",
    "@babel/plugin-proposal-class-properties": "^7.3.4",
    "@babel/preset-env": "^7.3.4",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.0.5",
    "eslint": "^5.15.1",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.12.4",
    "prettier": "^1.16.4",
    "source-map-loader": "^0.2.4",
    "stylelint": "^9.10.1",
    "stylelint-config-recommended": "^2.1.0",
    "webpack-cli": "^3.2.3",
    "webpack-dev-server": "^3.2.1"
  },
  "dependencies": {
    "babel-eslint": "^10.0.1",
    "babel-plugin-module-resolver": "^3.2.0",
    "css-loader": "^2.1.0",
    "file-loader": "^3.0.1",
    "html-webpack-plugin": "^3.2.0",
    "image-webpack-loader": "^4.6.0",
    "node-sass": "^4.11.0",
    "prop-types": "^15.7.2",
    "react": "^16.8.3",
    "react-dom": "^16.8.3",
    "react-hot-loader": "^4.7.2",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "webpack": "^4.29.6",
    "webpack-merge": "^4.2.1"
  },
  "bugs": {
    "url": "https://github.com/bernardodestefano/react-boilerplate/issues"
  },
  "homepage": "https://github.com/bernardodestefano/react-boilerplate#readme"
}
